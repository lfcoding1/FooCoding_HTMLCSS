!/bin/bash
# homework week 3
mkdir hworkwk3
cd hworkwk3
touch blank
for i in {1..5}; do echo hello >> greetings.txt; done
for i in {1..5}; do cp greetings.txt $i.txt; done
echo cat > pets.txt
echo dog >> pets.txt
echo hamster >> pets.txt
echo cat > commands.txt
echo ls >> commands.txt
echo pwd >> commands.txt
cat pets.txt | sort -u 
cat commands.txt | sort -u
cat commands.txt | sort -u > lovelyCommands.txt
cat pets.txt | sort -u > lovelyCommands.txt



